@model Haver_Niagara.Models.PartName

<form asp-controller="PartName" asp-action="Create" id="createPartNameForm">
    <div class="text-danger validation-summary-errors"><ul id="ErrorList"></ul></div>

    <div class="row">
        <div class="col-md-12">
            <div class="form-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="form-control" />
                <span asp-validation-for="Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" id="submitPartNameCreate" value="Create" class="btn btn-primary mt-3" />
            </div>
        </div>
    </div>
</form>

