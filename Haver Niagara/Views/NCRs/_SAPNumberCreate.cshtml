@model Haver_Niagara.Models.SAPNumber

<form asp-controller="SAPNumber" asp-action="Create" id="createSAPNumberForm">
    <div class="text-danger validation-summary-errors"><ul id="ErrorList"></ul></div>

    <div class="row">
        <div class="col-md-12">
            <div class="form-group">
                <label asp-for="Number" class="control-label"></label>
                <input asp-for="Number" class="form-control" />
                <span asp-validation-for="Number" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" id="submitSAPNumberCreate" value="Create" class="btn btn-primary mt-3" />
            </div>
        </div>
    </div>
</form>


